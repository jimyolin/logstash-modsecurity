{
	"template": "modsecurity*",
	"mappings": {
		"_default_" :{
			"_all":{
				"enabled": false
			},
			"properties":{
				"sourceIP-GEOIP":{
					"dynamic": true,
					"properties":{
						"location":{
							"type": "geo_point"
						},
						"ip":{
							"type": "ip"
						}
					},
					"type": "object"
				},
				
				"destIP":{
					"type": "ip"
				},
				"sourceIP":{
					"type":"ip"
				},
				"event_timestamp":{
					"type": "date"
				}
			}
		}
	}
}